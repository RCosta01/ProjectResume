<core:FragmentDefinition displayBlock="true" xmlns:core="sap.ui.core" height="100%" xmlns="sap.m">
    <SelectDialog busy="true" busyIndicatorDelay="0" updateFinished="_valueHelpUpdateFinished" id="pdp-libObjSearchEmp" title="{i18n>pdp.initiativeListTitle}" items="{ path: '/cust_pdpLibrary',
			sorter : {
				path : 'cust_initiativeType',
				group : true
			},
			groupHeaderFactory: '.getGroupHeader'
		}" search="_handleValueHelpSearch" confirm="_handleValueHelpClose" cancel="" multiSelect="true">
        <StandardListItem iconDensityAware="false" iconInset="false" title="{cust_objName}" description="{cust_objDesc}">
            <customData>
                <core:CustomData key="{cust_objLibraryId}" />
            </customData>
        </StandardListItem>
    </SelectDialog>
</core:FragmentDefinition>