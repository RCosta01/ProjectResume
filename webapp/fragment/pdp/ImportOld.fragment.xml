<core:FragmentDefinition displayBlock="true" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" height="100%" xmlns="sap.m">
    <Dialog busyIndicatorDelay="0" title="{i18n>pdp.importOldButtonText}" contentWidth="500px">
        <Panel class="confirmOverallPanel sapUiSmallMarginTop">
        <l:Grid defaultSpan="XL12 L12 M12 S12" class="createObj">
            <Title text="{i18n>pdp.importObjIni}" />
            <MultiInput width="100%" id="pdp-multiInputOld" class="sapUiTinyMarginTop sapUiSmallMarginBottom" valueHelpRequest="handleValueHelpOld">
                <core:Item key="{cust_objectiveId}" text="{cust_objectiveName}" />
            </MultiInput>
            <Title level="H2" text="{i18n>pdp.objCreationRealDate}" class="sapUiSmallMarginTop" />
            <DatePicker id="pdp-idDatePickerOld" displayFormat="dd-MM-yyyy" class="sapUiTinyMarginTop"/>
        </l:Grid>
        </Panel>
        <buttons>
            <Button text="{i18n>pdp.exitButtonText}" press="onExitOld" type="Reject" />
            <Button text="{i18n>pdp.addButtonText}" press="onConfirmOld" type="Accept" />
        </buttons>
    </Dialog>
</core:FragmentDefinition>